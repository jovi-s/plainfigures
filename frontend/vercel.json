{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "build": {
    "env": {
      "VITE_BACKEND_URL": "$VITE_BACKEND_URL"
    }
  },
  "rewrites": [
    {
      "source": "/app/(.*)",
      "destination": "/app/index.html"
    },
    {
      "source": "/app",
      "destination": "/app/index.html"
    }
  ]
}
